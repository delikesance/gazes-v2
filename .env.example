# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-minimum-32-characters
JWT_REFRESH_SECRET=your-super-secret-refresh-key-change-this-in-production-minimum-32-characters
JWT_EXPIRES_IN=7d
JWT_REFRESH_EXPIRES_IN=30d

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_JWT_SECRET=your-supabase-jwt-secret
POSTGRES_URL=postgres://postgres.your-project:your-password@aws-1-us-east-1.pooler.supabase.com:6543/postgres?sslmode=require
POSTGRES_PRISMA_URL=postgres://postgres.your-project:your-password@aws-1-us-east-1.pooler.supabase.com:6543/postgres?sslmode=require&pgbouncer=true
POSTGRES_USER=postgres
POSTGRES_HOST=db.your-project.supabase.co
POSTGRES_PASSWORD=your-password
POSTGRES_DATABASE=postgres

# Node Environment
NODE_ENV=development
