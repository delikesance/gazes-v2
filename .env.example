# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-minimum-32-characters
JWT_REFRESH_SECRET=your-super-secret-refresh-key-change-this-in-production-minimum-32-characters
JWT_EXPIRES_IN=7d
JWT_REFRESH_EXPIRES_IN=30d

# Supabase Configuration
gazes_SUPABASE_URL=https://your-project.supabase.co
gazes_SUPABASE_ANON_KEY=your-supabase-anon-key
gazes_SUPABASE_JWT_SECRET=your-supabase-service-role-key
gazes_SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
gazes_NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
gazes_NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
gazes_POSTGRES_URL=postgres://postgres.your-project:your-password@aws-1-us-east-1.pooler.supabase.com:6543/postgres?sslmode=require
gazes_POSTGRES_PRISMA_URL=postgres://postgres.your-project:your-password@aws-1-us-east-1.pooler.supabase.com:6543/postgres?sslmode=require&pgbouncer=true
gazes_POSTGRES_URL_NON_POOLING=postgres://postgres.your-project:your-password@aws-1-us-east-1.pooler.supabase.com:5432/postgres?sslmode=require
gazes_POSTGRES_USER=postgres
gazes_POSTGRES_HOST=db.your-project.supabase.co
gazes_POSTGRES_PASSWORD=your-password
gazes_POSTGRES_DATABASE=postgres

# Node Environment
NODE_ENV=development
